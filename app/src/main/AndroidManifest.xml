<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cool.test.mycollege">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission
        android:name="android.permission.WRITE_EXTERNAL_STORAGE"
        android:maxSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".EditAttendanceinWeek"
            android:label="@string/title_activity_attendance"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".AttendanceSettingsActivity"
            android:label="@string/attendance_settings"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".EditAttendanceinWeek" />
        </activity>
        <activity android:name=".AttendanceInfo">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".EditAttendanceinWeek" />
        </activity>

        <receiver
            android:name=".Helpers.MyReceiver"
            android:enabled="true"
            android:exported="false"></receiver>

        <service
            android:name=".Helpers.MyNewIntentService"
            android:exported="false"></service>

        <receiver
            android:name=".Helpers.CustomNotificationReceiver"
            android:enabled="true"
            android:exported="false"></receiver>

        <service
            android:name=".Helpers.MyNewCustomIntent"
            android:exported="false"></service>

        <activity
            android:name=".HomePage"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".EditTimeTable"
            android:label="@string/title_activity_edit_time_table"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".LostItemPreview" />
        <activity
            android:name=".login"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".signup"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Myuploads"
            android:label="@string/title_activity_myuploads"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CollegeInfo"
            android:label="INFO"
            android:parentActivityName=".HomePage"
            android:theme="@style/AppTheme.NoActionBar"
            />



        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/facebook_app_id" />

        <activity android:name=".StudyMaterialOnline" />
        <activity android:name=".Devs"></activity>
    </application>

</manifest>